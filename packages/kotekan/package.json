    "react-refresh": "0.14.0",
    "tailwindcss": "4.0.0-alpha.8",
    "typescript": "5.4.3",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/babel__core": "7.20.5",
    "@types/bun": "1.0.10",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "@types/react-refresh": "0.14.5",
    "@types/yargs": "17.0.32"
  }
