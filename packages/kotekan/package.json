    "react-error-boundary": "4.0.13",
    "react-native": "0.74.0-rc.5",
    "react-refresh": "0.14.0",
    "react-strict-dom": "0.0.4",
    "tailwindcss": "4.0.0-alpha.10",
    "typescript": "5.4.3",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/babel__core": "7.20.5",
    "@types/bun": "1.0.11",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.22",
    "@types/react-refresh": "0.14.5",
    "@types/yargs": "17.0.32"
