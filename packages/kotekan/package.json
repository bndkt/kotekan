    "@physis/react-server-dom-esm": "0.0.0-experimental-9c75cd5e8-20240318",
    "@react-native/babel-preset": "0.74.75",
    "@react-native/metro-config": "0.74.75",
    "@stylexjs/babel-plugin": "0.5.1",
    "@tailwindcss/cli": "4.0.0-alpha.8",
    "bun": "1.0.33",
    "isbot": "5.1.2",
    "mdx-annotations": "0.1.4",
    "metro": "0.80.7",
    "metro-core": "0.80.7",
