{
  "name": "kotekan",
  "version": "0.0.12",
  "description": "A modern React Server Components framework built on Bun.",
  "bin": {
    "kotekan": "./src/cli/cli.ts",
    "kotekan-dev": "./src/cli/dev.ts",
    "kotekan-jsx": "./src/cli/scripts/jsxServer.ts",
    "kotekan-ssr": "./src/cli/scripts/ssrServer.ts"
  },
  "keywords": [
    "react",
    "bun",
    "kotekan",
    "rsc"
  ],
  "homepage": "https://kotekan.dev",
  "bugs": {
    "url": "https://github.com/bndkt/kotekan/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bndkt/kotekan.git",
    "directory": "packages/kotekan"
  },
  "license": "MIT",
  "author": {
    "name": "Benedikt MÃ¼ller",
    "email": "hello@bndkt.com",
    "url": "http://bndkt.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./create": {
      "import": "./src/cli/commands/create.ts"
    },
    "./client/render": {
      "import": "./src/client/render.tsx"
    },
    "./client/hydrate": {
      "import": "./src/client/hydrate.tsx"
    }
  },
  "files": [
    "src/",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "bun ./src/scripts/build.ts",
    "compile": "bun build --compile ./src/cli/scripts/jsxServer.ts --outfile ./bin/kotekan-jsx",
    "lint": "biome lint ./src",
    "publish-dry": "npm publish --dry-run",
    "vendor": "bun ./src/scripts/vendor.ts"
  },
  "dependencies": {
    "@babel/core": "7.24.3",
    "@babel/plugin-syntax-flow": "7.24.1",
    "@babel/plugin-syntax-jsx": "7.24.1",
    "@babel/plugin-syntax-typescript": "7.24.1",
    "@babel/runtime": "7.24.1",
    "@mdx-js/mdx": "3.0.1",
    "@physis/react-server-dom-esm": "0.0.0-experimental-b1c2d14c9-20240326",
    "@react-native/babel-preset": "0.74.76",
    "@react-native/metro-config": "0.74.76",
    "@stylexjs/babel-plugin": "0.5.1",
    "@tailwindcss/cli": "4.0.0-alpha.9",
    "bun": "1.0.35",
    "isbot": "5.1.2",
    "metro": "0.80.7",
    "metro-core": "0.80.7",
    "react": "0.0.0-experimental-e373190fa-20240325",
