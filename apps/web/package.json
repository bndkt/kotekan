  },
  "devDependencies": {
    "@stylexjs/stylex": "0.5.1",
    "@types/bun": "1.0.8",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "typescript": "5.3.3"
  }
}